        $_subtotals = Mage::getSingleton('checkout/cart')->getQuote()->getTotals();
        $subtotal = $_subtotals["subtotal"]->getValue();
    ?>
    <?php $stringcart ='&nbsp;' . Mage::helper('checkout')->formatPrice($subtotal); ?>
